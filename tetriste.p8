function _init()

  bl_types = {
    { map={0,0},  size=2 },
    { map={3,0},  size=3 },
    { map={11,0}, size=3 },
    { map={15,0}, size=3 },
    { map={19,0}, size=3 },
    { map={23,0}, size=4 },
  }

  grid = {}
  grid.w = 8 * 10
  grid.h = 8 * 20
  grid.x = (128 - grid.w) / 2
  grid.y = 0

  shake = 0

  local bltype_i = flr(rnd(#bl_types))+1
  cur_block = mk_block(
    bltype_i,
    flr(grid.w/2/8) - bl_types[bltype_i].size / 2,
    flr(grid.h/2/8) - bl_types[bltype_i].size
  )
end

function _update()
 if shake > 0 then
  shake -= 0.1
 elseif shake != 0 then
  shake = 0
 end
 if btnp(0) then cur_block.r = 4 end
 if btnp(1) then cur_block.r = 2 end
 if btnp(2) then cur_block.r = 1 end
 if btnp(3) then cur_block.r = 3 end
 if btnp(4) then move_block(0,0,1) end

  --cur_block = mk_block(
  -- loop_int(
  --  cur_block.type_i, 1, 7, 1))
 if btnp(5) then move_block(0,0,-1) end
end

function _draw()
 cls()
 rectfill(0,0,127,127,0)
 rectfill(grid.x, 0, grid.x + grid.w, grid.h, 5)

 --local function eas(t)
 -- --t -= 1
 -- return t*t
 --end
 --local eas_shake=eas(shake)
 --rectfill(
 -- 0, 80,
 -- 127*(1-shake), 81, 1
 --)
 --rectfill(0, 83, 127*(1-eas_shake), 84, 1)

 -- draw_lines(grid.x, 0, shake)
 draw_block(grid, cur_block, 0)
 
 local i = mk_info()
  i('s '..shake)
  -- i('es '..eas_shake)
  i("type "..cur_block.type_i)
  i("x "..cur_block.x)
  i("y "..cur_block.y)
  i("r "..cur_block.r)
end

