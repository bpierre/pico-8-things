pico-8 cartridge // http://www.pico-8.com
version 7
__lua__

-- loop a value from f to t
-- d is direction (1, -1)
function loop_int(v, f, t, d)
 if v + d > t then
  return f
 end
 if v + d < f then
  return t
 end
 return v + d
end

function mk_block(type_i, x, y)
 local bl_type = bl_types[type_i]
 local block = {
  type_i = type_i,
  x = x,
  y = y,
  r = 1,
 }
 return block
end

function move_block(x, y, r)
 shake=1
 cur_block.x += x
 cur_block.y += y
 cur_block.r = loop_int(
  cur_block.r, 1, 4, r
 )
end

function mk_info()
 local row = 0
 return function(value)
  print(value,55,60+10*row,7)
  row += 1
 end
end

function easeinquint(t)
 return t*t*t*t*t
end

function block_type(block)
  return bl_types[block.type_i]
end

function draw_block(grid, block, shake)
  local bltype = block_type(block)
  local shakex = 0
  local shakey = 0
  local s = shake * 5

  if shake > 0 then
   shakex = rnd(s) - s/2
   shakey = rnd(s) - s/2
  end

  palt(0, false)
  map(
    bltype.map[1],
    bltype.map[2] + (
      bltype.size * (block.r - 1)
    ),
    grid.x + block.x * 8 + shakex,
    grid.y + block.y * 8 + shakey,
    bltype.size,
    bltype.size)
  palt()
end

